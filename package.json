    "css:lint": "stylelint 'site/sass/**/*.scss'",
    "dev": "run-p *:watch",
    "firebase:functions:log": "firebase functions:log",
    "firebase:functions:shell": "firebase functions:shell",
    "firebase:watch": "firebase serve --only functions --port=5010",

{
  "name": "chromeos.dev",
  "version": "1.0.0",
  "description": "My awesome site, built on Chrome OS's static site scaffold",
  "main": "src/js/main.js",
  "contributors": [
    "Sam Richard <snugug@google.com> (https://snugug.com)"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "ava:watch": "ava -w",
    "ava": "ava",
    "build:static": "gulp build",
    "build": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu run-p js static",
    "compile:optimize": "gulp html:optimize",
    "compile:test": "run-s 'compile -- --dryrun --quiet'",
    "compile:watch": "run-s 'compile -- --watch'",
    "compile": "cross-env NODE_ICU_DATA=node_modules/full-icu eleventy --quiet",
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu run-p *:watch",
    "gulp": "gulp",
    "js:lint": "eslint functions src lib gulpfile.js rollup.config.js rollup.sw.js",
    "js:sw": "rollup -c rollup.sw.js",
    "js:watch": "run-p 'js -- -w' 'js:sw -- -w'",
    "js": "rollup -c",
    "lint": "cross-env NODE_ENV=production run-p *:lint sass",
    "md:lint": "remark 'pages/*/!(style-guide)/**/*.md' -q -f",
    "postbuild": "cross-env NODE_ENV=production run-p compile:optimize js:sw",
    "postinstall": "patch-package",
    "prebuild": "del public",
    "prestart": "del public",
    "pretest": "run-s lint",
    "prettier:lint": "prettier -c './**/*.{html,md,js,json}'",
    "prettier": "prettier --write './**/*.{html,md,js,json}'",
    "sass": "gulp sass",
    "serve:public": "serve -l 5060",
    "serve:vrt": "serve -l 5566 .backstop",
    "start": "run-s dev",
    "static": "run-p compile build:static",
    "test:lint": "eslint tests",
    "test": "nyc ava",
    "vrt:approve": "node vrt.js approve",
    "vrt:reference": "node vrt.js reference",
    "vrt": "node vrt.js test"
  },
  "nyc": {
    "include": [
      "src/js/**/*.js",
      "lib/**/*.js"
    ],
    "all": true
  },
  "husky": {
    "hooks": {
      "pre-push": "run-s lint",
      "prepare-commit-msg": "exec < /dev/tty && git-cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/emoji-commit-helpers/commitizen.js"
    }
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@commitlint/cli": "^8.3.4",
    "@rollup/plugin-commonjs": "^11.1.0",
    "ajv": "^6.12.2",
    "ajv-merge-patch": "^4.1.0",
    "ava": "^3.9.0",
    "backstopjs": "^5.0.1",
    "breakpoint-sass": "^2.7.1",
    "cheerio": "^1.0.0-rc.3",
    "cli-spinner": "^0.2.10",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "del-cli": "^2.0.0",
    "emoji-commit-helpers": "^0.1.1",
    "emoji-regex": "^8.0.0",
    "eslint": "^6.5.1",
    "eslint-config-developit": "^1.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-spellcheck": "0.0.17",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "front-matter": "^3.2.1",
    "fs-extra": "^8.1.0",
    "fuse.js": "^3.4.6",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-sass-lint": "^1.4.0",
    "husky": "^3.0.7",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "js-yaml": "^3.14.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.kebabcase": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "remark": "^12.0.0",
    "remark-cli": "^8.0.0",
    "remark-frontmatter": "^2.0.0",
    "remark-lint": "^7.0.0",
    "replaceall": "^0.1.6",
    "rollup": "^1.31.1",
    "sass-toolkit": "^2.10.2",
    "serve": "^11.3.2",
    "serve-handler": "^6.1.3",
    "sinon": "^7.5.0",
    "string-strip-html": "^4.5.0",
    "through2": "^4.0.2",
    "unified-lint-rule": "^1.0.5",
    "unist-util-find": "^1.0.1",
    "unist-util-parents": "^1.0.3",
    "unist-util-visit": "^2.0.2",
    "yaml-unist-parser": "^1.2.1"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "comlink": "^4.3.0",
    "css-paint-polyfill": "^2.0.0",
    "dynamic-import-polyfill": "^0.1.1",
    "eleventy-plugin-nesting-toc": "^1.1.0",
    "full-icu": "^1.3.0",
    "gulp-multi-dest": "^1.3.7",
    "gulp-tap": "^2.0.0",
    "i18n-iso-countries": "^5.5.0",
    "iso-639-1": "^2.1.3",
    "lottie-web": "^5.7.0",
    "lunr": "^2.3.8",
    "patch-package": "^6.2.2",
    "resize-observer-polyfill": "^1.5.1",
    "service-worker-i18n-redirect": "^1.0.0",
    "service-worker-includes": "^1.0.1",
    "static-site-scaffold": "^4.2.1",
    "string": "^3.3.3",
    "workbox-broadcast-update": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3"
  },
  "private": true,
  "volta": {
    "node": "12.16.1"
  }
}

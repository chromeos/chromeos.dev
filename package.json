    "eslint-config-google": "^0.14",
    "eslint-config-prettier": "^6.15",
    "eslint-plugin-ava": "^8.0",
    "eslint-plugin-import": "^2.25",
    "eslint-plugin-prettier": "^3.4",
    "eslint-plugin-spellcheck": "^0.0",
    "esm": "^3.2",
    "express": "^4.17",
    "firebase-tools": "^9.23.1",
    "front-matter": "^3.2",
    "fs-extra": "^8.1",

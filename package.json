  "dependencies": {
    "comlink": "^4.3",
    "css-paint-polyfill": "^3.0",
    "dynamic-import-polyfill": "^0.1",
    "i18n-iso-countries": "^5.5",
    "iso-639-1": "^2.1",
    "lottie-web": "^5.8",
    "lunr": "^2.3",
    "resize-observer-polyfill": "^1.5",
    "service-worker-i18n-redirect": "^1.0.0",
    "service-worker-includes": "^1.0.1",
    "vite-plugin-pwa": "^0.11.11",
    "workbox-broadcast-update": "^6.4.2",
    "workbox-cacheable-response": "^6.4",
    "workbox-expiration": "^6.4",
    "workbox-google-analytics": "^6.4",
    "workbox-navigation-preload": "^6.4.2",
    "workbox-precaching": "^6.4",
    "workbox-recipes": "^6.4",
    "workbox-routing": "^6.4",
    "workbox-strategies": "^6.4"
  },
  "private": true,
  "volta": {
    "node": "14.18.2"
  },
  "lint-staged": {
    "(functions|site|lib)/**/*.js": "eslint --cache --fix",
    "vite.config.js": "eslint --cache --fix",
    "ava.config.cjs": "eslint --cache --fix",
    "site/*/!(style-guide)/**/*.md": "remark -q -f",
    "./**/*.{html,md,js,json}": "prettier -c --write",
    "tests/**/*.js": "eslint -c tests/.eslintrc.yml --cache --fix"
  }
}

    "image-size": "^1.0.0",
    "inquirer-autocomplete-prompt": "^1.4",
    "js-yaml": "^3.14",
    "lint-staged": "^12.1.5",
    "lodash.clonedeep": "^4.5",
    "lodash.get": "^4.4",
    "lodash.kebabcase": "^4.1",
    "node-fetch": "^2.6",
    "npm-run-all": "^4.1",
    "nyc": "^14.1",
    "patch-package": "^6.4",
    "posthtml-external-link": "^0.1",
    "posthtml-match-helper": "^1.0",
    "prettier": "^1.18",
    "remark": "^14.0",
    "remark-cli": "^8.0",

---
import { languages } from '$$sanity';

/**
 * Builds paths for content files
 * @return {object[]} Paths
 */
export function getStaticPaths() {
  return languages
    .filter((h) => h.code === 'en')
    .map((l) => ({
      params: {
        lang: l.code,
      },
    }));
}

const { lang } = Astro.params;
const url = `/${lang}/news`;
const content = `0; url='${url}'`;
---

<meta http-equiv="Refresh" content={content} />

{
  "name": "navy-cattle",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "pretest": "run-s lint",
    "test": "run-s build",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "prepare": "cd ../ && husky install ./cms/.husky",
    "lint": "run-p *:lint",
    "lint:staged": "lint-staged",
    "fix": "run-p *:fix",
    "js:lint": "eslint .",
    "js:fix": "eslint --fix .",
    "prettier:lint": "prettier -c .",
    "prettier:fix": "prettier --write .",
    "sanity": "sanity",
    "sanity:create:singletons": "sanity exec scripts/createSingletons.ts --with-user-token",
    "sanity:test:singletons": "TARGET=dev sanity exec scripts/createSingletons.ts --with-user-token"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@portabletext/react": "^3.0.15",
    "@sanity/code-input": "^4.1.3",
    "@sanity/document-internationalization": "^2.1.0",
    "@sanity/icons": "^2.11.6",
    "@sanity/ui": "^2.0.14",
    "@sanity/vision": "^3.36.3",
    "get-youtube-id": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-is": "^18.2.0",
    "react-lite-youtube-embed": "^2.4.0",
    "rxjs": "^7.8.1",
    "sanity": "^3.36.4",
    "sanity-plugin-computed-field": "^2.0.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=8.6.0"
  },
  "volta": {
    "node": "18.16.0",
    "pnpm": "8.6.0"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,json}": "eslint",
    "**/*": "prettier -c"
  }
}
